                    <trans-unit resname="RECOMMEND_A" id="studyactions.types.RECOMMEND_A">
                        <source>Application or plugin is recommended by study</source>
                        <target>L'applicazione o il plugin è raccomandato dallo studio</target>
                    </trans-unit>
                    <trans-unit resname="DATALINK" id="studyactions.types.DATALINK">
                        <source>Application shares data to study participants</source>
                        <target>L'applicazione condivide i dati con i partecipanti allo studio</target>
                    </trans-unit>
                </group>
                <group resname="types_short" id="studyactions.types_short">
                    <trans-unit resname="OFFER_P" id="studyactions.types_short.OFFER_P">
                        <source>Suggest participation</source>
                        <target>Suggerire la partecipazione</target>
                    </trans-unit>
                    <trans-unit resname="AUTOADD_A" id="studyactions.types_short.AUTOADD_A">
                        <source>Add automatically</source>
                        <target>Aggiungere automaticamente</target>
                    </trans-unit>
                    <trans-unit resname="DATALINK" id="studyactions.types_short.DATALINK">
                        <source>Data Sharing</source>
                        <target>Condivisione dei dati</target>
                    </trans-unit>
                </group>
            </group>
            <group resname="studyoverview" id="studyoverview">
                <group resname="confirm" id="studyoverview.confirm">
                    <trans-unit resname="validation" id="studyoverview.confirm.validation">
                        <source>Setup cannot be changed anymore. Validation process by MIDATA will be started.</source>
                    </trans-unit>
                    <trans-unit resname="end_participant_search" id="studyoverview.confirm.end_participant_search">
                        <source>No additional users will be able to participate anymore.</source>
                    </trans-unit>
                    <trans-unit resname="finish_execution" id="studyoverview.confirm.finish_execution">
                        <source>All study data will be frozen at this moment.</source>
                    </trans-unit>
                    <trans-unit resname="delete" id="studyoverview.confirm.delete">
                        <source>The research project will be completely wiped from the platform.</source>
                    </trans-unit>
                    <trans-unit resname="abort" id="studyoverview.confirm.abort">
                        <source>All study data will be frozen at this moment. This project will be marked as failed.</source>
                    </trans-unit>
                    <trans-unit resname="are_you_sure" id="studyoverview.confirm.are_you_sure">
                        <source>This operation cannot be undone. Are you sure?</source>
                    </trans-unit>
                </group>
            </group>
