            <group resname="login" id="login">
                <trans-unit resname="password" id="login.password">
                    <source>Password</source>
                </trans-unit>
                <trans-unit resname="info1" id="login.info1">
                    <source>Dear study participant</source>
                    <target>Caro partecipante allo studio</target>
                </trans-unit>
                <trans-unit resname="info2" id="login.info2">
                    <source>You already have a MIDATA account, log in with your email address and password.</source>
                    <target>Hai gi√† un account MIDATA, accedi con il tuo indirizzo email e la tua password.</target>
                </trans-unit>
                <trans-unit resname="info3" id="login.info3">
                    <source>Your personal account gives you an overview of your study participation.</source>
                    <target>Il tuo account personale ti fornisce una panoramica della partecipazione allo studio.</target>
                </trans-unit>
                <trans-unit resname="info4" id="login.info4">
                    <source>Dear visitor</source>
                    <target>Caro visitatore</target>
                </trans-unit>
                <trans-unit resname="info5" id="login.info5">
                    <source>You do not yet have a MIDATA account and do not participate in a study?</source>
                    <target>Non hai ancora un account MIDATA e non partecipi ad uno studio?</target>
                </trans-unit>
                <trans-unit resname="info6" id="login.info6">
                    <source>Join the</source>
                    <target>Partecipa al progetto</target>
                </trans-unit>
                <trans-unit resname="info6b" id="login.info6b">
                    <source>Ally Science</source>
                    <target>Ally Science</target>
                </trans-unit>
                <trans-unit resname="info6c" id="login.info6c">
                    <source>project!</source>
                    <target>!</target>
                </trans-unit>
            </group>
            <group resname="changeaddress" id="changeaddress">
                <trans-unit resname="success" id="changeaddress.success">
                    <source>Your account information has been updated.</source>
                </trans-unit>
            </group>
            <group resname="user" id="user">
                <trans-unit resname="wipe_account_btn" id="user.wipe_account_btn">
                    <source>Delete Account</source>
                </trans-unit>
            </group>
            <group resname="records" id="records">
                <trans-unit resname="just_name" id="records.just_name">
                    <source>{{name}}</source>
                </trans-unit>
            </group>
            <group resname="oauth2" id="oauth2">
                <trans-unit resname="sign_up_btn" id="oauth2.sign_up_btn">
                    <source>Sign up</source>
                    <target>Iscriviti</target>
                </trans-unit>
            </group>
            <group resname="dashboard" id="dashboard">
                <trans-unit resname="social" id="dashboard.social">
                    <source>Social</source>
                </trans-unit>
            </group>
            <group resname="taskdetails" id="taskdetails">
                <trans-unit resname="plugin" id="taskdetails.plugin">
                    <source>Plugin:</source>
                </trans-unit>
            </group>
            <group resname="auditlog" id="auditlog">
                <trans-unit resname="via" id="auditlog.via">
                    <source>via</source>
                </trans-unit>
                <trans-unit resname="MEMBER" id="auditlog.MEMBER">
                    <source>Account</source>
                </trans-unit>
            </group>
            <group resname="accountwipe" id="accountwipe">
                <trans-unit resname="title" id="accountwipe.title">
                    <source>Close and Delete MIDATA Account</source>
                </trans-unit>
                <trans-unit resname="hello" id="accountwipe.hello">
                    <source>Dear {{name}},</source>
                </trans-unit>
                <trans-unit resname="paragraph1" id="accountwipe.paragraph1">
                    <source>You would like to delete your MIDATA account irrevocably? That is of course possible.</source>
                </trans-unit>
                <trans-unit resname="paragraph2" id="accountwipe.paragraph2">
                    <source>Please read the following information first:</source>
                </trans-unit>
                <trans-unit resname="point1" id="accountwipe.point1">
                    <source>If you delete your MIDATA account your personal data will no longer be available.</source>
                </trans-unit>
                <trans-unit resname="point2" id="accountwipe.point2">
                    <source>Data already shared for research will remain available to the studies it has been shared to up to the time of account deletion. No new data will be shared.</source>
                </trans-unit>
                <trans-unit resname="point3" id="accountwipe.point3">
                    <source>MIDATA is not able to recover your deleted account.</source>
                </trans-unit>
                <trans-unit resname="warning" id="accountwipe.warning">
                    <source>Warning</source>
                </trans-unit>
                <trans-unit resname="final" id="accountwipe.final">
                    <source>This action cannot be undone.</source>
                </trans-unit>
                <trans-unit resname="paragraph3" id="accountwipe.paragraph3">
                    <source>If you really want to delete your MIDATA account enter your password for confirmation.</source>
                </trans-unit>
                <trans-unit resname="password" id="accountwipe.password">
                    <source>Password</source>
                </trans-unit>
                <trans-unit resname="reason" id="accountwipe.reason">
                    <source>Reason (optional)</source>
                </trans-unit>
                <trans-unit resname="reason2" id="accountwipe.reason2">
                    <source>Please tell us why you want to delete your account. We are continously trying to improve the service.</source>
                </trans-unit>
                <trans-unit resname="wipe_btn" id="accountwipe.wipe_btn">
                    <source>Close and Delete MIDATA Account</source>
                </trans-unit>
                <trans-unit resname="error" id="accountwipe.error">
                    <source>Password not correct. Account not deleted.</source>
                </trans-unit>
            </group>
            <group resname="serviceleave" id="serviceleave">
                <trans-unit resname="title" id="serviceleave.title">
                    <source>Done with your session?</source>
                </trans-unit>
                <trans-unit resname="thankyou" id="serviceleave.thankyou">
                    <source>Thank you for your help!</source>
                </trans-unit>
                <trans-unit resname="goodbye_btn" id="serviceleave.goodbye_btn">
                    <source>End session and close window</source>
                </trans-unit>
                <trans-unit resname="logoutreturn" id="serviceleave.logoutreturn">
                    <source>You will be redirected to the calling page.</source>
                </trans-unit>
                <trans-unit resname="return_btn" id="serviceleave.return_btn">
                    <source>Logout and continue</source>
                </trans-unit>
            </group>
        </body>
